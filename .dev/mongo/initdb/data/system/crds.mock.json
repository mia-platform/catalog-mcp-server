  {
    "apiVersion": "mia-platform.eu/v1alpha1",
    "kind": "CustomResourceDefinition",
    "metadata": {
      "name": "templates.mia-platform.eu"
    },
    "spec": {
      "group": "mia-platform.eu",
      "names": {
        "kind": "Template",
        "plural": "templates",
